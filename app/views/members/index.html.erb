<h1>Registered Members</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @members.each do |member| %>
      <tr>
        <td><%= member.full_name %></td>
        <td><%= member.email %></td>
        <td>
          <%= link_to 'View', member_path(member) %> |
          <%= link_to 'Edit', edit_member_path(member) %> |
          <%= button_to 'Delete', member_path(member), method: :delete, data: { confirm: 'Are you sure?' } %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
